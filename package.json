{
  "name": "directus-extension-stripe-bundle",
  "version": "1.0.0-alpha.1",
  "type": "module",
  "author": {
    "name": "Pawe≈Ç Romanowski",
    "url": "https://pawel-romanowski.pl/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nexthis/directus-stripe"
  },
  "keywords": [
    "typescript",
    "plugin",
    "stripe",
    "directus",
    "directus-extension",
    "directus-custom-bundle"
  ],
  "description": "Receive payments on your Directus commerce application using Stripe.",
  "icon": "extension",
  "directus:extension": {
    "host": "^10.1.11",
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "hook",
        "name": "stripe-hook",
        "source": "src/hook/index.ts"
      },
      {
        "type": "endpoint",
        "name": "stripe-endpoint",
        "source": "src/endpoint/index.ts"
      }
    ]
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "add": "directus-extension add",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "stripe": "^14.7.0",
    "stripe": "^13.8.0"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "^10.3.3",
    "@directus/types": "^11.0.6",
    "@types/node": "^20.10.2",
    "typescript": "^5.2.2",
    "vitepress": "^1.0.0-rc.31",
    "vitepress": "^1.0.0-rc.20",
    "stripe": "^14.16.0"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "^10.3.1",
    "@directus/types": "^11.0.4",
    "@types/node": "^20.11.17",
    "typescript": "^5.2.2",
    "vitepress": "^1.0.0-rc.42"
  }
}